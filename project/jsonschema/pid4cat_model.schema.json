{
    "$defs": {
        "Agent": {
            "additionalProperties": false,
            "description": "Data class for a person who plays a role relative to PID creation or curation.",
            "properties": {
                "affiliation_ror": {
                    "description": "The ROR of the agent's affiliation.",
                    "pattern": "^https:\\/\\/ror\\.org\\/0[a-hj-km-np-tv-z|0-9]{6}[0-9]{2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "email_address": {
                    "description": "Email address of the agent that created or modified the PID record.",
                    "pattern": "^\\S+@[\\S+\\.]+\\S+",
                    "type": "string"
                },
                "name": {
                    "description": "The name of the agent that created or modified the PID record.",
                    "type": "string"
                },
                "orcid": {
                    "description": "The ORCID of the person",
                    "pattern": "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[0-9X]$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "role": {
                    "$ref": "#/$defs/Pid4CatAgentRole",
                    "description": "The role of the agent relative to the resource"
                }
            },
            "required": [
                "name",
                "email_address",
                "role"
            ],
            "title": "Agent",
            "type": "object"
        },
        "ArkIdentifier": {
            "additionalProperties": false,
            "description": "An ARK (Archival Resource Key).",
            "properties": {
                "identifier": {
                    "description": "The identifier in recommended notation.",
                    "pattern": "^ark:\\/\\d{5}/.*$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "resolving_url": {
                    "description": "The URL that resolves the identifier.",
                    "pattern": "^https?:\\/\\/.*\\/ark:\\/\\d{5}/.*$",
                    "type": "string"
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "ArkIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "resolving_url",
                "type"
            ],
            "title": "ArkIdentifier",
            "type": "object"
        },
        "CHANGES": {
            "additionalProperties": false,
            "description": "The data element in the handle API for the change log.",
            "properties": {
                "data": {
                    "$ref": "#/$defs/HdlDataLog",
                    "description": "The data in the handle record."
                },
                "index": {
                    "description": "The index of the handle record.",
                    "maximum": 16,
                    "minimum": 16,
                    "type": "integer"
                },
                "timestamp": {
                    "description": "The iso datetime for the last update of the handle data.",
                    "format": "date-time",
                    "type": "string"
                },
                "ttl": {
                    "description": "A time to live in seconds for the handle record. Typically: 86400 => 1 day",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "type": {
                    "description": "The type of handledata stored in the handle record.",
                    "enum": [
                        "CHANGES"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "index",
                "data",
                "timestamp",
                "type"
            ],
            "title": "CHANGES",
            "type": "object"
        },
        "ChangeLogField": {
            "description": "The field of the pid4cat record that was changed.",
            "enum": [
                "STATUS",
                "LANDING_PAGE",
                "RESOURCE_INFO",
                "RELATED_IDS",
                "CONTACT",
                "LICENSE",
                "SCHEMA_VER"
            ],
            "title": "ChangeLogField",
            "type": "string"
        },
        "DoiIdentifier": {
            "additionalProperties": false,
            "description": "A digital object identifier (DOI).",
            "properties": {
                "identifier": {
                    "description": "The identifier in recommended notation.",
                    "pattern": "^10\\.\\d{4,}\\/.*$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "resolving_url": {
                    "description": "The URL that resolves the identifier.",
                    "pattern": "^https:\\/\\/doi\\.org\\/10.*$",
                    "type": "string"
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "DoiIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "resolving_url",
                "type"
            ],
            "title": "DoiIdentifier",
            "type": "object"
        },
        "EMAIL": {
            "additionalProperties": false,
            "description": "The data element in the handle API for the contact email.",
            "properties": {
                "data": {
                    "$ref": "#/$defs/HdlDataContact",
                    "description": "The data in the handle record."
                },
                "index": {
                    "description": "The index of the handle record.",
                    "maximum": 10,
                    "minimum": 10,
                    "type": "integer"
                },
                "timestamp": {
                    "description": "The iso datetime for the last update of the handle data.",
                    "format": "date-time",
                    "type": "string"
                },
                "ttl": {
                    "description": "A time to live in seconds for the handle record. Typically: 86400 => 1 day",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "type": {
                    "description": "The type of handledata stored in the handle record.",
                    "enum": [
                        "EMAIL"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "index",
                "data",
                "timestamp",
                "type"
            ],
            "title": "EMAIL",
            "type": "object"
        },
        "ExampleIdentifier": {
            "additionalProperties": false,
            "description": "An example.org test identifier.",
            "properties": {
                "identifier": {
                    "description": "The identifier in recommended notation.",
                    "pattern": "^ex:.*$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "resolving_url": {
                    "description": "The URL that resolves the identifier.",
                    "pattern": "^https?:\\/\\/(.+\\.)?example.(org|com)\\/.*$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "ExampleIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "type"
            ],
            "title": "ExampleIdentifier",
            "type": "object"
        },
        "GtinIdentifier": {
            "additionalProperties": false,
            "description": "A Global Trade Item Number (GTIN) previously called European Article Number (EAN) often encoded as EAN13 barcode. The identifier is used to identify products. GTINs don't have a resolvable URL.",
            "properties": {
                "identifier": {
                    "description": "The identifier in recommended notation.",
                    "pattern": "^\\d{13}$",
                    "type": "string"
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "GtinIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "identifier",
                "type"
            ],
            "title": "GtinIdentifier",
            "type": "object"
        },
        "HandleAPIRecord": {
            "additionalProperties": false,
            "description": "A class representing a handle record query response of the REST (json) API of a handle server.",
            "properties": {
                "handle": {
                    "description": "The handle of the pid4cat record.",
                    "pattern": "^\\d{2}\\.T?\\d{4,}\\/.*$",
                    "type": "string"
                },
                "responseCode": {
                    "description": "The response code of the handle API.",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "values": {
                    "description": "The values of the pid4cat record.",
                    "items": {
                        "anyOf": [
                            {
                                "$ref": "#/$defs/HandleRecord"
                            },
                            {
                                "$ref": "#/$defs/URL"
                            },
                            {
                                "$ref": "#/$defs/EMAIL"
                            },
                            {
                                "$ref": "#/$defs/STATUS"
                            },
                            {
                                "$ref": "#/$defs/SCHEMAVER"
                            },
                            {
                                "$ref": "#/$defs/METADATALICENSE"
                            },
                            {
                                "$ref": "#/$defs/RESOURCE"
                            },
                            {
                                "$ref": "#/$defs/RELATED"
                            },
                            {
                                "$ref": "#/$defs/CHANGES"
                            }
                        ]
                    },
                    "type": "array"
                }
            },
            "required": [
                "handle",
                "values"
            ],
            "title": "HandleAPIRecord",
            "type": "object"
        },
        "HandleIdentifier": {
            "additionalProperties": false,
            "description": "A handle identifier.",
            "properties": {
                "identifier": {
                    "description": "The identifier in recommended notation.",
                    "pattern": "^\\d{2}\\.T?\\d{4,}\\/.*$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "resolving_url": {
                    "description": "The URL that resolves the identifier.",
                    "pattern": "^https:\\/\\/hdl\\.handle\\.net\\/\\d{2}\\.T?\\d{4,}\\/.*$",
                    "type": "string"
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "HandleIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "resolving_url",
                "type"
            ],
            "title": "HandleIdentifier",
            "type": "object"
        },
        "HandleRecord": {
            "additionalProperties": false,
            "description": "A base class for handle-data classes that represent a handle record in the same way as in the REST (json) API of a handle server.",
            "properties": {
                "timestamp": {
                    "description": "The iso datetime for the last update of the handle data.",
                    "format": "date-time",
                    "type": "string"
                },
                "ttl": {
                    "description": "A time to live in seconds for the handle record. Typically: 86400 => 1 day",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "type": {
                    "description": "The type of handledata stored in the handle record.",
                    "enum": [
                        "HandleRecord"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "timestamp",
                "type"
            ],
            "title": "HandleRecord",
            "type": "object"
        },
        "HandleRecordContainer": {
            "additionalProperties": false,
            "description": "A container for all HandleRecords.",
            "properties": {
                "contains_pids": {
                    "description": "The HandleRecords contained in the container.",
                    "items": {
                        "$ref": "#/$defs/HandleAPIRecord"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                }
            },
            "title": "HandleRecordContainer",
            "type": "object"
        },
        "HdlDataContact": {
            "additionalProperties": false,
            "description": "The data class for the handle-record contact email.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "description": "The value of the handle data.",
                    "pattern": "^\\S+@[\\S+\\.]+\\S+",
                    "type": "string"
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataContact",
            "type": "object"
        },
        "HdlDataLicense": {
            "additionalProperties": false,
            "description": "The data class for the PID metadata license.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "const": "CC0-1.0",
                    "description": "The value of the handle data.",
                    "type": "string"
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataLicense",
            "type": "object"
        },
        "HdlDataLog": {
            "additionalProperties": false,
            "description": "The data class for the change log.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "description": "The value of the handle data.",
                    "items": {
                        "$ref": "#/$defs/LogRecord"
                    },
                    "type": "array"
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataLog",
            "type": "object"
        },
        "HdlDataRelated": {
            "additionalProperties": false,
            "description": "The data class for related identifiers.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "description": "The value of the handle data.",
                    "items": {
                        "$ref": "#/$defs/Pid4CatRelation"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                }
            },
            "title": "HdlDataRelated",
            "type": "object"
        },
        "HdlDataResourceInfo": {
            "additionalProperties": false,
            "description": "The data class for the resource info.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "$ref": "#/$defs/ResourceInfo",
                    "description": "The value of the handle data."
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataResourceInfo",
            "type": "object"
        },
        "HdlDataSchemaVer": {
            "additionalProperties": false,
            "description": "The data class for the schema version.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "description": "The value of the handle data.",
                    "pattern": "^v\\d+\\.\\d+\\.\\d+$",
                    "type": "string"
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataSchemaVer",
            "type": "object"
        },
        "HdlDataStatus": {
            "additionalProperties": false,
            "description": "The data class for the PID status information.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "$ref": "#/$defs/Pid4CatStatus",
                    "description": "The value of the handle data."
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataStatus",
            "type": "object"
        },
        "HdlDataUrl": {
            "additionalProperties": false,
            "description": "The data class for the redirect url.",
            "properties": {
                "format": {
                    "const": "string",
                    "description": "The format of the handle data.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "value": {
                    "description": "The value of the handle data.",
                    "pattern": "^https?:\\/\\/.*$",
                    "type": "string"
                }
            },
            "required": [
                "value"
            ],
            "title": "HdlDataUrl",
            "type": "object"
        },
        "LogRecord": {
            "additionalProperties": false,
            "description": "Data class for a change log of modification made on a pid4cat handle record starting from its registration.",
            "properties": {
                "changed_field": {
                    "$ref": "#/$defs/ChangeLogField",
                    "description": "The field that was changed."
                },
                "datetime_log": {
                    "description": "The date and time of a log record.",
                    "format": "date-time",
                    "type": "string"
                },
                "description": {
                    "description": "A human-readable description for a resource.",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "has_agent": {
                    "$ref": "#/$defs/Agent",
                    "description": "The person who registered or modified the PID record."
                }
            },
            "required": [
                "datetime_log",
                "has_agent",
                "changed_field"
            ],
            "title": "LogRecord",
            "type": "object"
        },
        "METADATALICENSE": {
            "additionalProperties": false,
            "description": "The data element in the handle API for the PID metadata license.",
            "properties": {
                "data": {
                    "$ref": "#/$defs/HdlDataLicense",
                    "description": "The data in the handle record."
                },
                "index": {
                    "description": "The index of the handle record.",
                    "maximum": 13,
                    "minimum": 13,
                    "type": "integer"
                },
                "timestamp": {
                    "description": "The iso datetime for the last update of the handle data.",
                    "format": "date-time",
                    "type": "string"
                },
                "ttl": {
                    "description": "A time to live in seconds for the handle record. Typically: 86400 => 1 day",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "type": {
                    "description": "The type of handledata stored in the handle record.",
                    "enum": [
                        "METADATA_LICENSE"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "index",
                "data",
                "timestamp",
                "type"
            ],
            "title": "METADATALICENSE",
            "type": "object"
        },
        "MediaTypesEnum": {
            "description": "IANA media types are used to specify the type of data.",
            "enum": [
                "application/epub+zip",
                "application/json",
                "application/ld+json",
                "application/octet-stream",
                "application/pdf",
                "application/vnd.eln+zip",
                "application/vnd.openxmlformats-officedocument.presentationml.presentation",
                "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
                "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
                "application/xml",
                "application/yaml",
                "application/zip",
                "image/gif",
                "image/jpeg",
                "image/png",
                "image/svg+xml",
                "image/tiff",
                "image/webp",
                "text/csv",
                "text/html",
                "text/javascript",
                "text/markdown",
                "text/plain",
                "text/tab-separated-values",
                "text/turtle",
                "text/xml",
                "video/mp4",
                "video/webm"
            ],
            "title": "MediaTypesEnum",
            "type": "string"
        },
        "Pid4CatAgentRole": {
            "description": "The role of an agent relative to the resource.",
            "enum": [
                "TRUSTEE",
                "OWNER"
            ],
            "title": "Pid4CatAgentRole",
            "type": "string"
        },
        "Pid4CatRecord": {
            "additionalProperties": false,
            "description": "A class representing pid4cat identifiers with its metadata as objects. This is a neutral object-oriented representation that does not mirror the record structure of the handle system but is provided as representation that is more convenient to use in programming languages.",
            "properties": {
                "change_log": {
                    "description": "The change log of the pid4cat record. The change log contains information about the changes made to the pid4cat record.",
                    "items": {
                        "$ref": "#/$defs/LogRecord"
                    },
                    "type": "array"
                },
                "curation_contact": {
                    "description": "The email address of the person responsible for the curation of the pid4cat record.",
                    "pattern": "^\\S+@[\\S+\\.]+\\S+",
                    "type": "string"
                },
                "landing_page_url": {
                    "description": "The URL of the landing page of the resource identified by the PID.",
                    "pattern": "^https?:\\/\\/.*$",
                    "type": "string"
                },
                "metadata_license": {
                    "const": "CC0-1.0",
                    "description": "The license of the metadata of the pid4cat record. The license is set to \"CC0-1.0\" by default.",
                    "type": "string"
                },
                "related_identifiers": {
                    "description": "The related identifiers of the pid4cat record. The related identifiers are used to link the pid4cat record to other resources.",
                    "items": {
                        "anyOf": [
                            {
                                "$ref": "#/$defs/RelatedIdentifier"
                            },
                            {
                                "$ref": "#/$defs/PurlIdentifier"
                            },
                            {
                                "$ref": "#/$defs/DoiIdentifier"
                            },
                            {
                                "$ref": "#/$defs/HandleIdentifier"
                            },
                            {
                                "$ref": "#/$defs/ArkIdentifier"
                            },
                            {
                                "$ref": "#/$defs/UrnIdentifier"
                            },
                            {
                                "$ref": "#/$defs/GtinIdentifier"
                            },
                            {
                                "$ref": "#/$defs/ExampleIdentifier"
                            }
                        ]
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "resource_info": {
                    "$ref": "#/$defs/ResourceInfo",
                    "description": "The resource info of the pid4cat record. The resource info contains information about the resource identified by the PID."
                },
                "schema_version": {
                    "$ref": "#/$defs/HdlDataSchemaVer",
                    "description": "The version of the pid4cat-model used to create the pid4cat record."
                },
                "status": {
                    "$ref": "#/$defs/Pid4CatStatus",
                    "description": "The status of the pid4cat record. The status is set to \"SUBMITTED\" when the handle is reserved but the resource is not yet linked. The status is set to \"REGISTERED\" when the handle is linked to a concrete resource."
                }
            },
            "required": [
                "landing_page_url",
                "status",
                "schema_version",
                "metadata_license",
                "curation_contact",
                "resource_info",
                "change_log"
            ],
            "title": "Pid4CatRecord",
            "type": "object"
        },
        "Pid4CatRelation": {
            "additionalProperties": false,
            "description": "Data class for a relation to another resource identified by a pid4cat handle or another PID type.",
            "properties": {
                "datetime_log": {
                    "description": "The date and time of a log record.",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "related_identifier": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/RelatedIdentifier"
                        },
                        {
                            "$ref": "#/$defs/PurlIdentifier"
                        },
                        {
                            "$ref": "#/$defs/DoiIdentifier"
                        },
                        {
                            "$ref": "#/$defs/HandleIdentifier"
                        },
                        {
                            "$ref": "#/$defs/ArkIdentifier"
                        },
                        {
                            "$ref": "#/$defs/UrnIdentifier"
                        },
                        {
                            "$ref": "#/$defs/GtinIdentifier"
                        },
                        {
                            "$ref": "#/$defs/ExampleIdentifier"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The related identifier."
                },
                "relation_type": {
                    "$ref": "#/$defs/RelationType",
                    "description": "Relation type between the resources."
                }
            },
            "title": "Pid4CatRelation",
            "type": "object"
        },
        "Pid4CatStatus": {
            "description": "The usage status of the pid4cat record.",
            "enum": [
                "SUBMITTED",
                "REGISTERED",
                "OBSOLETED",
                "DEPRECATED"
            ],
            "title": "Pid4CatStatus",
            "type": "string"
        },
        "PurlIdentifier": {
            "additionalProperties": false,
            "description": "A PURL (permanent uniform resource locator).",
            "properties": {
                "resolving_url": {
                    "description": "The URL that resolves the identifier.",
                    "pattern": "^https:\\/\\/(purl|pida|w3id)\\.org\\/.*$",
                    "type": "string"
                },
                "type": {
                    "description": "The type of the identifier.",
                    "enum": [
                        "PurlIdentifier"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "resolving_url",
                "type"
            ],
            "title": "PurlIdentifier",
            "type": "object"
        },
        "RELATED": {
            "additionalProperties": false,
            "description": "The data element in the handle API for related identifiers.",
            "properties": {
                "data": {
                    "$ref": "#/$defs/HdlDataRelated",
                    "description": "The data in the handle record."
                },
                "index": {
                    "description": "The index of the handle record.",
                    "maximum": 15,
                    "minimum": 15,
                    "type": "integer"
                },
