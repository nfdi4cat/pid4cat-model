pid4cat_model:Agent a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Agent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:email_address ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:affiliation_ror ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:orcid ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[0-9X]$" ] ) ] ) ] ;
            owl:onProperty pid4cat_model:orcid ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:role ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:role ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:affiliation_ror ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:orcid ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty pid4cat_model:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:email_address ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ) ] ;
            owl:onProperty pid4cat_model:email_address ],
        [ a owl:Restriction ;
            owl:allValuesFrom pid4cat_model:Pid4CatAgentRole ;
            owl:onProperty pid4cat_model:role ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Uri [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^https:\\/\\/ror\\.org\\/0[a-hj-km-np-tv-z|0-9]{6}[0-9]{2}$" ] ) ] ) ] ;
            owl:onProperty pid4cat_model:affiliation_ror ] ;
    skos:definition "Data class for a person who plays a role relative to PID creation or curation." ;
    skos:exactMatch prov1:Agent ;
    skos:inScheme <https://w3id.org/nfdi4cat/pid4cat-model> .

pid4cat_model:ArkIdentifier a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ArkIdentifier" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:resolving_url ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:resolving_url ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:identifier ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:identifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Uri [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^https?:\\/\\/.*\\/ark:\\/\\d{5}/.*$" ] ) ] ) ] ;
            owl:onProperty pid4cat_model:resolving_url ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:String [ a rdfs:Datatype ;
                                owl:onDatatype xsd:string ;
                                owl:withRestrictions ( [ xsd:pattern "^ark:\\/\\d{5}/.*$" ] ) ] ) ] ;
            owl:onProperty pid4cat_model:identifier ],
        pid4cat_model:RelatedIdentifier ;
    skos:definition "An ARK (Archival Resource Key)." ;
    skos:inScheme <https://w3id.org/nfdi4cat/pid4cat-model> .

pid4cat_model:CHANGES a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CHANGES" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:data ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( linkml:Integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 16 ] ) ] [ a rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:maxInclusive 16 ] ) ] ) ] ;
            owl:onProperty pid4cat_model:index ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:data ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty pid4cat_model:index ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:index ],
        [ a owl:Restriction ;
            owl:allValuesFrom pid4cat_model:HdlDataLog ;
            owl:onProperty pid4cat_model:data ],
        pid4cat_model:HandleRecord ;
    skos:definition "The data element in the handle API for the change log." ;
    skos:inScheme <https://w3id.org/nfdi4cat/pid4cat-model> .

