            owl:onProperty pid4cat_model:relation_type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:datetime_log ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:relation_type ],
        [ a owl:Restriction ;
