        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:datetime_log ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty pid4cat_model:changed_field ],
