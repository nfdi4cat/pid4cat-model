            owl:onProperty pid4cat_model:related_identifier ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty pid4cat_model:related_identifier ] ;
    skos:definition "A relation between pid4cat handles or between a pid4cat handle and other resources identified by a PID." ;
    skos:inScheme <https://w3id.org/nfdi4cat/pid4cat-model> .

