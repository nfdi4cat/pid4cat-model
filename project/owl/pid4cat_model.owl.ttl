pid4cat_model:HandleRecord a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "HandleRecord" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty pid4cat_model:index ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
