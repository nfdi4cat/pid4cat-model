pid4cat_model:ArkIdentifier a sh:NodeShape ;
    sh:closed true ;
    sh:description "An ARK (Archival Resource Key)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The type of the identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path pid4cat_model:type ],
        [ sh:datatype <xsd:anyURI> ;
            sh:description "The URL that resolves the identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path pid4cat_model:resolving_url ;
            sh:pattern "^https?:\\/\\/.*\\/ark:\\/\\d{5}/.*$" ],
        [ sh:datatype <xsd:string> ;
            sh:description "The identifier in recommended notation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:identifier ;
            sh:pattern "^ark:\\/\\d{5}/.*$" ] ;
    sh:targetClass pid4cat_model:ArkIdentifier .

