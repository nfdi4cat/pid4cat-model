pid4cat_model:Pid4CatRecord a sh:NodeShape ;
    sh:closed true ;
    sh:description "A class representing pid4cat identifiers with its metadata as objects. This is a neutral object-oriented representation that does not mirror the record structure of the handle system but is provided as representation that is more convenient to use in programming languages." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The version of the pid4cat-model used to create the pid4cat record." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path pid4cat_model:schema_version ],
        [ sh:datatype <xsd:string> ;
            sh:description "The URL of the landing page of the resource identified by the PID." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:landing_page_url ;
            sh:pattern "^https?:\\/\\/.*$" ],
        [ sh:class pid4cat_model:ResourceInfo ;
            sh:description "The resource info of the pid4cat record. The resource info contains information about the resource identified by the PID." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path pid4cat_model:resource_info ],
        [ sh:datatype <xsd:string> ;
            sh:description "The license of the metadata of the pid4cat record. The license is set to \"CC0-1.0\" by default." ;
            sh:in ( "CC0-1.0" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path pid4cat_model:metadata_license ],
        [ sh:datatype <xsd:string> ;
            sh:description "The email address of the person responsible for the curation of the pid4cat record." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path pid4cat_model:curation_contact ;
            sh:pattern "^\\S+@[\\S+\\.]+\\S+" ],
        [ sh:description "The status of the pid4cat record. The status is set to \"SUBMITTED\" when the handle is reserved but the resource is not yet linked. The status is set to \"REGISTERED\" when the handle is linked to a concrete resource." ;
            sh:in ( voc4cat:0005052 voc4cat:0005053 voc4cat:0005054 voc4cat:0005055 ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path pid4cat_model:status ],
        [ sh:class pid4cat_model:LogRecord ;
            sh:description "The change log of the pid4cat record. The change log contains information about the changes made to the pid4cat record." ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path pid4cat_model:change_log ],
        [ sh:class pid4cat_model:Pid4CatRelation ;
            sh:description "The related identifiers for the pid4cat record. The related identifiers are used to link the pid4cat record to other resources." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path pid4cat_model:related_identifiers ] ;
    sh:targetClass pid4cat_model:Pid4CatRecord .

