    sh:description "The data element in the handle API for the resource info." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The format of the handle data." ;
            sh:in ( "string" ) ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:format ],
        [ sh:class pid4cat_model:ResourceInfo ;
            sh:description "The value of the handle data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path pid4cat_model:value ] ;
    sh:targetClass pid4cat_model:HdlDataResourceInfo .

pid4cat_model:HdlDataSchemaVer a sh:NodeShape ;
    sh:closed true ;
    sh:description "The data element in the handle API for the schema version." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The value of the handle data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path pid4cat_model:value ;
            sh:pattern "^v\\d+\\.\\d+\\.\\d+$" ],
        [ sh:datatype <xsd:string> ;
            sh:description "The format of the handle data." ;
            sh:in ( "string" ) ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:format ] ;
    sh:targetClass pid4cat_model:HdlDataSchemaVer .

pid4cat_model:HdlDataStatus a sh:NodeShape ;
    sh:closed true ;
    sh:description "The data element in the handle API for the status." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The value of the handle data." ;
            sh:in ( voc4cat:0005052 voc4cat:0005053 voc4cat:0005054 voc4cat:0005055 ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path pid4cat_model:value ],
        [ sh:datatype <xsd:string> ;
            sh:description "The format of the handle data." ;
            sh:in ( "string" ) ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:format ] ;
    sh:targetClass pid4cat_model:HdlDataStatus .

pid4cat_model:HdlDataUrl a sh:NodeShape ;
    sh:closed true ;
    sh:description "The data element in the handle API for the redirect url." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The value of the handle data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path pid4cat_model:value ;
            sh:pattern "^https?:\\/\\/.*$" ],
        [ sh:datatype <xsd:string> ;
            sh:description "The format of the handle data." ;
            sh:in ( "string" ) ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:format ] ;
    sh:targetClass pid4cat_model:HdlDataUrl .

pid4cat_model:LogRecord a sh:NodeShape ;
    sh:closed true ;
    sh:description "A log record for changes made in a pid4cat handle record starting from registration." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description for a resource." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path pid4cat_model:description ],
        [ sh:datatype <xsd:dateTime> ;
            sh:description "The date and time of a log record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:datetime_log ],
        [ sh:description "The field that was changed" ;
            sh:in ( "STATUS" "LANDING_PAGE" "RESOURCE_INFO" "RELATED_IDS" "CONTACT" "LICENSE" "SCHEMA_VER" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path pid4cat_model:changed_field ],
        [ sh:class prov1:Agent ;
            sh:description "The person who registered or modified the PID record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path pid4cat_model:has_agent ] ;
    sh:targetClass pid4cat_model:LogRecord .

pid4cat_model:Pid4CatRelation a sh:NodeShape ;
    sh:closed true ;
    sh:description "A relation between pid4cat handles or between a pid4cat handle and other resources identified by a PID." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:dateTime> ;
            sh:description "The date and time of a log record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path pid4cat_model:datetime_log ],
        [ sh:description "Relation type between the resources." ;
            sh:in ( voc4cat:0005019 voc4cat:0005020 voc4cat:0005021 voc4cat:0005022 voc4cat:0005023 voc4cat:0005024 voc4cat:0005025 voc4cat:0005026 voc4cat:0005027 voc4cat:0005028 voc4cat:0005029 voc4cat:0005030 voc4cat:0005031 voc4cat:0005032 voc4cat:0005033 voc4cat:0005034 voc4cat:0005035 voc4cat:0005036 voc4cat:0005037 voc4cat:0005038 voc4cat:0005039 voc4cat:0005040 voc4cat:0005041 voc4cat:0005042 voc4cat:0005043 voc4cat:0005044 voc4cat:0005045 voc4cat:0005046 voc4cat:0005047 voc4cat:0005048 voc4cat:0005049 voc4cat:0005050 ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path pid4cat_model:relation_type ],
        [ sh:class pid4cat_model:RelatedIdentifier ;
            sh:description "The related identifier." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path pid4cat_model:related_identifier ] ;
    sh:targetClass pid4cat_model:Pid4CatRelation .

pid4cat_model:RelatedIdentifier a sh:NodeShape ;
    sh:closed true ;
    sh:description "A class for all types pf related identifiers." ;
    sh:ignoredProperties ( pid4cat_model:resolving_url rdf:type pid4cat_model:identifier ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "The type of the identifier." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:type ] ;
    sh:targetClass pid4cat_model:RelatedIdentifier .

pid4cat_model:RepresentationVariant a sh:NodeShape ;
    sh:closed true ;
    sh:description "A representation of the resource in other media types than text/html which is the default for landing_page_url." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The media type of the representation as defined by [IANA](https://www.iana.org/assignments/media-types/media-types.xhtml)" ;
            sh:in ( "application/epub+zip" "application/json" "application/ld+json" "application/octet-stream" "application/pdf" "application/vnd.eln+zip" "application/vnd.openxmlformats-officedocument.presentationml.presentation" "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" "application/vnd.openxmlformats-officedocument.wordprocessingml.document" "application/xml" "application/yaml" "application/zip" "image/gif" "image/jpeg" "image/png" "image/svg+xml" "image/tiff" "image/webp" "text/csv" "text/html" "text/javascript" "text/markdown" "text/plain" "text/tab-separated-values" "text/turtle" "text/xml" "video/mp4" "video/webm" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path pid4cat_model:media_type ],
        [ sh:datatype <xsd:integer> ;
            sh:description "The size of the representation in bytes." ;
            sh:maxCount 1 ;
            sh:minInclusive 0 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path pid4cat_model:size ],
        [ sh:datatype <xsd:string> ;
            sh:description "The encoding of the representation. https://encoding.spec.whatwg.org/#names-and-labels" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path pid4cat_model:encoding_format ],
        [ sh:datatype <xsd:anyURI> ;
            sh:description "The URL of the representation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:url ] ;
    sh:targetClass pid4cat_model:RepresentationVariant .

pid4cat_model:ResourceInfo a sh:NodeShape ;
    sh:closed true ;
    sh:description "Data object to hold information about the resource and its representation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype <xsd:string> ;
            sh:description "A human-readable description for a resource." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path pid4cat_model:description ],
        [ sh:class pid4cat_model:RepresentationVariant ;
            sh:description "The representations of the resource in other media types than text/html." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path pid4cat_model:representation_variants ],
        [ sh:description "The category of the resource." ;
            sh:in ( voc4cat:0005012 voc4cat:0005013 voc4cat:0005014 voc4cat:0005015 voc4cat:0005016 voc4cat:0005017 ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path pid4cat_model:resource_category ],
        [ sh:datatype <xsd:string> ;
            sh:description "A human-readable name for a resource." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:label ] ;
    sh:targetClass pid4cat_model:ResourceInfo .

prov1:Agent a sh:NodeShape ;
    sh:closed true ;
    sh:description "Person who plays a role relative to PID creation or curation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The role of the agent relative to the resource" ;
            sh:in ( "TRUSTEE" "OWNER" ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path pid4cat_model:role ],
        [ sh:datatype <xsd:string> ;
            sh:description "The name of the agent that created or modified the PID record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path pid4cat_model:name ],
        [ sh:datatype <xsd:string> ;
            sh:description "Email address of the agent that created or modified the PID record." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path pid4cat_model:email ;
            sh:pattern "^\\S+@[\\S+\\.]+\\S+" ],
        [ sh:datatype <xsd:anyURI> ;
            sh:description "The ROR of the agent's affiliation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path pid4cat_model:affiliation_ror ],
        [ sh:datatype <xsd:string> ;
            sh:description "The ORCID of the person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path pid4cat_model:orcid ] ;
    sh:targetClass prov1:Agent .
